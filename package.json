{
  "name": "scott-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:cf": "wrangler dev --live-reload",
    "build": "next build",
    "build:cf": "opennextjs-cloudflare build",
    "migrate:local": "wrangler d1 migrations apply DB --local",
    "migrate:staging": "wrangler d1 migrations apply DB --remote --env staging",
    "migrate:production": "wrangler d1 migrations apply DB --remote --env production",
    "deploy:staging": "npm run migrate:staging && npm run build:cf && wrangler deploy --env staging",
    "deploy:production": "npm run migrate:production && npm run build:cf && wrangler deploy --env production",
    "cf-typegen": "wrangler types",
    "lint": "next lint"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@uiw/react-md-editor": "^4.0.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jose": "^6.1.3",
    "lucide-react": "^0.562.0",
    "next": "14.2.35",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwind-merge": "^3.4.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^1.14.8",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "18.3.27",
    "@types/react-dom": "18.3.7",
    "@types/uuid": "^10.0.0",
    "eslint": "8.57.1",
    "eslint-config-next": "14.2.35",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "wrangler": "^4.57.0"
  }
}

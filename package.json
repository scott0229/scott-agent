    "react-dom": "18.3.1",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^1.14.8",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "18.3.27",
    "@types/react-dom": "18.3.7",
    "eslint": "8.57.1",
